public class planets {
 String name;
 Double mass;
 Double xpos;
 Double ypos;
 Double xvel;
 Double yvel;
 public planets(String Name,Double Mass, Double x, Double y, Double Xvel, Double Yvel){
  name = Name;
  mass = Mass;
  xpos = x;
  ypos = y;
  xvel = Xvel;
  yvel = Yvel; 
  
 }

 public double getXvel(){
 return xvel;
 }
 
 public Double getYvel(){
 return yvel;
 } 

 public double getXpos(){
 return xpos;
 }
 
 public Double getYpos(){
 return ypos;
 }
 
 public void setXpos(Double newX){
 xpos = newX;
 }
 
 public void setYpos(Double newY){
 ypos = newY;
 }

 public void setXvel(Double newX){
 xvel = newX;
 }
 
 public void setYvel(Double newY){
 yvel = newY;
 }
 
 public Double getMass(){
 return mass;
 }
 
 public String getName(){
 return name;
 }

 }



