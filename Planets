public class planets {
 String name;
 Double mass;
 Double xpos;
 Double ypos;
 public planets(String name,Double Mass, Double x, Double y){
  name = Name;
  mass = Mass;
  xpos = x;
  ypos = y;
  
 }
 
 public double getXpos(){
 return xpos;
 }
 
 public Double getYpos(){
 return ypos;
 }
 
 public void setXpos(Double new){
 xpos = new;
 }
 
 public void setYpos(Double new){
 ypos = new;
 }
 
 public Double getMass(){
 return mass;
 }
 
 public String getName(){
 return namep;
 }
 
 
 
 
 
 }
