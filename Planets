public class planets {
 String name;
 Double mass;
 Double xpos;
 Double ypos;
 public planets(String Name,Double Mass, Double x, Double y){
  name = Name;
  mass = Mass;
  xpos = x;
  ypos = y;
  
 }
 
 public double getXpos(){
 return xpos;
 }
 
 public Double getYpos(){
 return ypos;
 }
 
 public void setXpos(Double newX){
 xpos = newX;
 }
 
 public void setYpos(Double newY){
 ypos = newY;
 }
 
 public Double getMass(){
 return mass;
 }
 
 public String getName(){
 return name;
 }

 }
